{
    "name": "learning/timezones",
    "description": "Demo-package to show time",
    "authors": [
        {
            "name": "dan",
            "email": "dan007@mailinator.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "packages/learning/timezones",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "learning/timezones": "dev"
    },
    "autoload": {
        "psr-4": {
            "Learning\\Timezones\\":"learning/timezones/src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Learning\\Timezones\\TimezonesServiceProvider"
            ]
        }
    }
}